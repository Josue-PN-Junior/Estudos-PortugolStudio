programa
{
	
	funcao inicio()
	{
		inteiro n1, n2
		escreva("Insira o primeiro valor: ")
		leia(n1)
		escreva("Insira o segundo valor: ")
		leia(n2)
		se (n1>n2) {
			escreva("O primeiro valor é maior!")
		} senao se (n2>n1) {
			escreva("O segundo valor é maior!")
		} senao {
			escreva("Os valores são iguais.")
		}
	}
}
